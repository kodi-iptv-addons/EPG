8\u0446\u0435\u0439\u0441\u043a\u043e\u0435 \u0440\u0430\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043b\u0435\u043d\u0438\u044f, \u0430 \u0432\u0442\u043e\u0440\u0430\u044f \u0434\u043e\u0432\u043e\u0434\u0438\u0442 \u0435\u0433\u043e \u0434\u043e \u043b\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0442\u043e\u0447\u043a\u0438 \u0432 \u0437\u0430\u043b\u0435 \u0441\u0443\u0434\u0430. \u041e\u0442\u043b\u0438\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u043c\u0438 \u0447\u0435\u0440\u0442\u0430\u043c\u0438 \u0441\u0435\u0440\u0438\u0430\u043b\u0430 \u0432\u0441\u0435\u0433\u0434\u0430 \u044f\u0432\u043b\u044f\u043b\u0438\u0441\u044c \u043d\u0438\u0437\u043a\u043e\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u043a \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u044f\u043c \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u044b\u0445 \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0445 \u043b\u0438\u0446 \u0438 \u043d\u0435\u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0430\u043a\u0442\u0451\u0440\u0441\u043a\u043e\u0433\u043e \u0441\u043e\u0441\u0442\u0430\u0432\u0430, \u0447\u0442\u043e, \u043e\u0434\u043d\u0430\u043a\u043e, \u043d\u0435 \u043f\u043e\u043c\u0435\u0448\u0430\u043b\u043e \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u043c \u0433\u0435\u0440\u043e\u044f\u043c \u0441\u0435\u0440\u0438\u0430\u043b\u0430, \u0442\u0430\u043a\u0438\u043c, \u043a\u0430\u043a \u0434\u0435\u0442\u0435\u043a\u0442\u0438\u0432 \u041b\u0435\u043d\u043d\u0438 \u0411\u0440\u0438\u0441\u043a\u043e \u0438 \u043f\u0440\u043e\u043a\u0443\u0440\u043e\u0440 \u0414\u0436\u0435\u043a \u041c\u0430\u043a\u043a\u043e\u0439, \u0432\u043e\u0439\u0442\u0438 \u0432 \u0447\u0438\u0441\u043b\u043e \u0441\u0430\u043c\u044b\u0445 \u0437\u0430\u043c\u0435\u0442\u043d\u044b\u0445 \u0438 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0445 \u0442\u0435\u043b\u0435\u0432\u0438\u0437\u0438\u043e\u043d\u043d\u044b\u0445 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u0436\u0435\u0439.  \u0412 1999 \u0433\u043e\u0434\u0443 \u0443 \"\u0417\u0430\u043a\u043e\u043d\u0430 \u0438 \u043f\u043e\u0440\u044f\u0434\u043a\u0430\" \u043f\u043e\u044f\u0432\u0438\u043b\u0441\u044f \u043f\u0435\u0440\u0432\u044b\u0439 \u0441\u043f\u0438\u043d-\u043e\u0444\u0444 - \"\u0417\u0430\u043a\u043e\u043d \u0438 \u043f\u043e\u0440\u044f\u0434\u043e\u043a: \u0421\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u043a\u043e\u0440\u043f\u0443\u0441\". \u042d\u0442\u043e\u0442 \u0441\u0435\u0440\u0438\u0430\u043b, \u043f\u043e\u0441\u0432\u044f\u0449\u0451\u043d\u043d\u044b\u0439 \u043e\u0441\u043e\u0431\u043e\u043c\u0443 \u043f\u043e\u0434\u0440\u0430\u0437\u0434\u0435\u043b\u0435\u043d\u0438\u044e \u043d\u044c\u044e-\u0439\u043e\u0440\u043a\u0441\u043a\u043e\u0439 \u043f\u043e\u043b\u0438\u0446\u0438\u0438 \u043f\u043e \u0440\u0430\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044e \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043b\u0435\u043d\u0438\u0439, \u0441\u043e\u0432\u0435\u0440\u0448\u0451\u043d\u043d\u044b\u0445 \u043d\u0430 \u0441\u0435\u043a\u0441\u0443\u0430\u043b\u044c\u043d\u043e\u0439 \u043f\u043e\u0447\u0432\u0435, \u0431\u043e\u043b\u0435\u0435 \u0431\u043b\u0438\u0437\u043e\u043a \u043a \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u044f\u043c \u0434\u0435\u0442\u0435\u043a\u0442\u0438\u0432\u043d\u043e\u0439 \u043f\u043e\u043b\u0438\u0446\u0435\u0439\u0441\u043a\u043e\u0439 \u0434\u0440\u0430\u043c\u044b \u0441 \u0444\u043e\u043a\u0443\u0441\u0438\u0440\u043e\u0432\u043a\u043e\u0439 \u043d\u0430 \u0432\u0435\u0434\u0443\u0449\u0438\u0445 \u0440\u0430\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u043e\u043b\u0438\u0446\u0435\u0439\u0441\u043a\u0438\u0445. \u041f\u043e\u043c\u0438\u043c\u043e \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u043d\u0430\u0445\u043e\u0434\u044f\u0449\u0438\u0445\u0441\u044f \u0432 \u0446\u0435\u043d\u0442\u0440\u0435 \u0441\u044e\u0436\u0435\u0442\u0430 \u0433\u043b\u0430\u0432\u043d\u044b\u0445 \u0433\u0435\u0440\u043e\u0435\u0432, \u0434\u0435\u0442\u0435\u043a\u0442\u0438\u0432\u043e\u0432 \u042d\u043b\u043b\u0438\u043e\u0442\u0430 \u0421\u0442\u0435\u0439\u0431\u043b\u0435\u0440\u0430 \u0438 \u041e\u043b\u0438\u0432\u0438\u0438 \u0411\u0435\u043d\u0441\u043e\u043d, \u043e\u0441\u043e\u0431\u044b\u0439 \u0438\u043d\u0442\u0435\u0440\u0435\u0441 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0414\u0436\u043e\u043d \u041c\u0430\u043d\u0447.  \u041f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0439 \u0438\u0437 \u0434\u043e \u0441\u0438\u0445 \u043f\u043e\u0440 \u0432\u044b\u0445\u043e\u0434\u044f\u0449\u0438\u0445 \u0441\u043f\u0438\u043d-\u043e\u0444\u0444\u043e\u0432 - \"\u0417\u0430\u043a\u043e\u043d \u0438 \u043f\u043e\u0440\u044f\u0434\u043e\u043a: \u041f\u0440\u0435\u0441\u0442\u0443\u043f\u043d\u044b\u0435 \u043d\u0430\u043c\u0435\u0440\u0435\u043d\u0438\u044f\", \u0441\u0442\u0430\u0440\u0442\u043e\u0432\u0430\u0432\u0448\u0438\u0439 \u0432 2001 \u0433\u043e\u0434\u0443. \u0417\u0434\u0435\u0441\u044c \u0444\u043e\u043a\u0443\u0441 \u0432\u043f\u0435\u0440\u0432\u044b\u0435 \u0441\u043c\u0435\u0449\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u044c \u0438 \u043c\u043e\u0442\u0438\u0432\u044b \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043d\u0438\u043a\u0430, \u0430 \u0437\u0440\u0438\u0442\u0435\u043b\u044c \u0441\u0430\u043c \u043c\u043e\u0436\u0435\u0442 \u0432\u044b\u0441\u0442\u0440\u043e\u0438\u0442\u044c \u0441\u0432\u043e\u044e \u0432\u0435\u0440\u0441\u0438\u044e \u0441\u043e\u0431\u044b\u0442\u0438\u0439 \u043d\u0430 \u043e\u0441\u043d\u043e\u0432\u0435 \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u043c\u044b\u0445 \u0432 \u043d\u0430\u0447\u0430\u043b\u0435 \u043a\u0430\u0436\u0434\u043e\u0439 \u0441\u0435\u0440\u0438\u04