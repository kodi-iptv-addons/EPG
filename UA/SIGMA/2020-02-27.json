u0438 \u0434\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u0432\u0440\u0435\u043c\u044f \u0432 \u043f\u0440\u0438\u0451\u043c\u043d\u043e\u043c \u043e\u0442\u0434\u0435\u043b\u0435\u043d\u0438\u0438. \u0421 \u0442\u0435\u0445 \u043f\u043e\u0440 \u041e\u043a\u0441\u0430\u043d\u0430 \u043d\u0430\u0447\u0430\u043b\u0430 \u0440\u0430\u0437\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u0442\u044c \u043f\u0440\u043e\u0435\u043a\u0442 \u0440\u0435\u0444\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0431\u043e\u043b\u044c\u043d\u0438\u0446 \u0441\u043a\u043e\u0440\u043e\u0439 \u043f\u043e\u043c\u043e\u0449\u0438. \u0427\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0438 \u0438\u0437 \u043c\u0438\u043d\u0438\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430 \u0437\u0434\u0440\u0430\u0432\u043e\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f \u043e\u0442\u043c\u0430\u0445\u0438\u0432\u0430\u043b\u0438\u0441\u044c \u043e\u0442 \u0435\u0451 \u0438\u043d\u0438\u0446\u0438\u0430\u0442\u0438\u0432, \u043f\u043e\u043a\u0430 \u043a\u0430\u0440\u0434\u0438\u043e\u0445\u0438\u0440\u0443\u0440\u0433 \u043d\u0435 \u0434\u043e\u043d\u0435\u0441\u043b\u0430 \u0441\u0432\u043e\u0438 \u0438\u0434\u0435\u0438 \u0434\u043e \u0432\u044b\u0441\u043e\u043a\u043e\u043f\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u043b\u0438\u0446\u0430. \u041e\u043d\u0438 \u0432\u044b\u043d\u0443\u0436\u0434\u0435\u043d\u044b \u0431\u044b\u043b\u0438 \u043f\u043e\u0439\u0442\u0438 \u043d\u0430 \u0443\u0441\u0442\u0443\u043f\u043a\u0438. \u0427\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0438 \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u044e\u0442 \u041e\u043a\u0441\u0430\u043d\u0435 \u0432\u043e\u0437\u0433\u043b\u0430\u0432\u0438\u0442\u044c \u043f\u0440\u0438\u0451\u043c\u043d\u043e\u0435 \u043e\u0442\u0434\u0435\u043b\u0435\u043d\u0438\u0435 13-\u0439 \u0431\u043e\u043b\u044c\u043d\u0438\u0446\u044b \u0441\u043a\u043e\u0440\u043e\u0439 \u043f\u043e\u043c\u043e\u0449\u0438 \u0438 \u043f\u0440\u043e\u0434\u0435\u043c\u043e\u043d\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u044d\u0444\u0444\u0435\u043a\u0442\u0438\u0432\u043d\u043e\u0441\u0442\u044c \u043f\u0440\u043e\u0435\u043a\u0442\u0430 \u043d\u0430 \u043f\u0440\u0430\u043a\u0442\u0438\u043a\u0435.  \u041a\u043e\u0432\u0430\u043b\u044c\u0447\u0443\u043a \u0431\u0440\u043e\u0441\u0430\u0435\u0442 \u0441\u0432\u043e\u0451 \u043f\u0440\u0435\u0441\u0442\u0438\u0436\u043d\u043e\u0435 \u0438 \u0432\u044b\u0441\u043e\u043a\u043e\u043e\u043f\u043b\u0430\u0447\u0438\u0432\u0430\u0435\u043c\u043e\u0435 \u043c\u0435\u0441\u0442\u043e \u0438 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0435\u0442 \u0432\u044b\u0437\u043e\u0432. \u0415\u0451 \u0432\u043e\u0437\u043b\u044e\u0431\u043b\u0435\u043d\u043d\u044b\u0439 \u0410\u043d\u0442\u043e\u043d \u043d\u0435\u0434\u043e\u0432\u043e\u043b\u0435\u043d \u044d\u0442\u0438\u043c \u0440\u0435\u0448\u0435\u043d\u0438\u0435\u043c, \u043a\u043e\u0442\u043e\u0440\u043e\u0435 \u043e\u043d \u0441\u0447\u0438\u0442\u0430\u0435\u0442 \u0431\u0435\u0441\u043f\u0435\u0440\u0441\u043f\u0435\u043a\u0442\u0438\u0432\u043d\u044b\u043c. \u0412\u043c\u0435\u0441\u0442\u0435 \u0441 \u043a\u0430\u0440\u0434\u0438\u043e\u0445\u0438\u0440\u0443\u0440\u0433\u043e\u043c \u0432 \u043f\u0440\u0438\u0451\u043c\u043d\u043e\u0435 \u043e\u0442\u0434\u0435\u043b\u0435\u043d\u0438\u0435 \u043e\u0442\u043f\u0440\u0430\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u0442\u0440\u0430\u0432\u043c\u0430\u0442\u043e\u043b\u043e\u0433 \u041d\u0430\u0437\u0430\u0440 \u0411\u0443\u043b\u044c\u0431\u0430 \u0438 \u043d\u0435\u0432\u0440\u043e\u043b\u043e\u0433 \u0411\u043e\u0440\u0438\u0441 \u0417\u0443\u0435\u0432 - \u0431\u044b\u0432\u0448\u0438\u0439 \u043c\u0443\u0436 \u041a\u043e\u0432\u0430\u043b\u044c\u0447\u0443\u043a. \u041e\u043a\u0441\u0430\u043d\u0430 \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0438\u0432\u0430\u0435\u0442 \u043d\u0430 \